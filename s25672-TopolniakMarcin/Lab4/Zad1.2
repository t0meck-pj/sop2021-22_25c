#include <stdlib.h>
#include <stdio.h>
int main()
{
	int n;
	int i = 0;
	int suma = 0;
	int liczba = 0;
	printf("Podaj ilość liczb: ");
	if (!fscanf(stdin, "%d", &n))
	{
		printf("Podana liczba jest niepoprawna\n");
		return -1;
	}
	for (; i < n; i++)
	{
		printf("Podaj liczbe[%d]: ", i+1);
		if (!fscanf(stdin, "%d", &liczba))
		{
			printf("Podana liczba jest niepoprawna\n");
			return -1;
		}
		suma += liczba;
	}
	printf("Suma: %d\n", suma);
	return suma;
}
